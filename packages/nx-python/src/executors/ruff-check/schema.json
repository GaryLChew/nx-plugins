{
  "$schema": "http://json-schema.org/schema",
  "title": "Ruff Check executor",
  "description": "",
  "type": "object",
  "properties": {
    "lintFilePatterns": {
      "type": "array",
      "description": "File patterns to check",
      "items": {
        "type": "string"
      }
    },
    "__unparsed__": {
      "hidden": true,
      "type": "array",
      "items": {
        "type": "string"
      },
      "$default": {
        "$source": "unparsed"
      },
      "x-priority": "internal"
    }
  },
  "required": ["lintFilePatterns"]
}
